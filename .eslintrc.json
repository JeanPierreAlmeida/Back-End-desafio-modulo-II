{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "comma-dangle": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off"
    }
}
